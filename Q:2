CREATE TABLE IF NOT EXISTS Employees (
    Emp_Id INTEGER PRIMARY KEY AUTOINCREMENT,
    Name CHAR(20) NOT NULL,
    Age INT NOT NULL,
    R_address VCHAR(25),
    Salary FLOAT NOT NULL
);

